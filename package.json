{
  "name": "severa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "echo [ Generating production build ] && npm run env -s && ng build --aot --prod",
    "start": "echo [ Starting application ] && npm run build && npm run env -s && npm run serve:express",
    "serve:express": "echo [ Initializing express server ] && node server.js ",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "postinstall": "npm run prettier -s && npm run build -s",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.2.2",
    "@angular/material-moment-adapter": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@angular/service-worker": "^7.2.2",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@ngx-translate/core": "^11.0.1",
    "angular-svg-icon": "^7.1.1",
    "chart.js": "^2.7.3",
    "core-js": "^2.6.3",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.0.0-alpha.12",
    "jquery": "^3.3.1",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.10",
    "material-design-icons-iconfont": "^4.0.3",
    "moment": "^2.24.0",
    "ng2-charts": "^1.6.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.2",
    "@types/jspdf": "^1.2.2",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.4.4",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.2",
    "puppeteer": "^1.4.0",
    "stylelint": "~9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.1",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.2.4"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
